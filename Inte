local RunService = game:GetService("RunService")

local plr = game.Players.LocalPlayer
local chr = plr.Character or plr.CharacterAdded:Wait()
local hrp = chr:WaitForChild("HumanoidRootPart")
local hum = chr:WaitForChild("Humanoid")
local rs = game:GetService("ReplicatedStorage")
local tweenService = game:GetService("TweenService")
local dmg
local done = false

local part = Instance.new("Part")
part.Size = Vector3.new(5, 5, 5)

if workspace.CurrentRooms:FindFirstChild(game.ReplicatedStorage.GameData.LatestRoom.Value):FindFirstChild(game.ReplicatedStorage.GameData.LatestRoom.Value) then
part.CFrame = workspace.CurrentRooms:FindFirstChild(game.ReplicatedStorage.GameData.LatestRoom.Value):FindFirstChild(game.ReplicatedStorage.GameData.LatestRoom.Value).CFrame * CFrame.new(0,4,0)
else
part.CFrame = hrp.CFrame * CFrame.new(0,0,-10)
end

part.Anchored = true
part.Transparency = 1
part.CanCollide = false
part.CastShadow = false
part.Parent = workspace.CurrentRooms

local sound = Instance.new("Sound")
sound.Parent = game.Workspace
sound.SoundId = "rbxassetid://715673747"
sound.Volume = 5
sound.PlaybackSpeed = 0.95
sound.Looped = false
sound:Play()

local billboard = Instance.new("BillboardGui")
billboard.AlwaysOnTop = true
billboard.Size = UDim2.new(8, 0, 8, 0)
billboard.SizeOffset = Vector2.new(0, 0.2)
billboard.Parent = part
billboard.Adornee = part

local imageLabel = Instance.new("ImageLabel")
imageLabel.Size = UDim2.new(1, 0, 1, 0)
imageLabel.BackgroundTransparency = 1
imageLabel.Image = "rbxassetid://105649190399055"
imageLabel.Parent = billboard
wait(5)

dmg = RunService.Heartbeat:Connect(function()
if chr:GetAttribute("Hiding") then
dmg:Disconnect()
local tw = tweenService:Create(sound, TweenInfo.new(5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {PlaybackSpeed = 0})
tw:Play()
tw.Completed:Wait()
part:Destroy()
sound:Destroy()
done = true
end
end)

wait(15)
if done then return end
hum.Health -= 9999
game:GetService("ReplicatedStorage").GameStats["Player_"..plr.Name].Total.DeathCause.Value = "The 1nT3Gr1Ty"
local es = tweenService:Create(sound, TweenInfo.new(5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {PlaybackSpeed = 0})
es:Play()
es.Completed:Wait()
part:Destroy()
sound:Destroy()
